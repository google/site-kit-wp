{
  "name": "google/google-site-kit",
  "description": "Site Kit is is a one-stop solution for WordPress users to use everything Google has to offer to make them successful on the web.",
  "version": "1.0.0-beta.1",
  "license": "Apache-2.0",
  "type": "wordpress-plugin",
  "homepage": "https://sitekit.withgoogle.com",
  "require-dev": {
    "phpunit/phpunit": "^4",
    "squizlabs/php_codesniffer": "^3.3",
    "wp-coding-standards/wpcs": "^1",
    "phpcompatibility/php-compatibility": "^9",
    "dealerdirect/phpcodesniffer-composer-installer": "0.4.4"
  },
  "require": {
    "php": ">=5.4",
    "google/apiclient": "^2.0",
    "guzzlehttp/guzzle": "~5.3.3"
  },
  "autoload": {
    "psr-4": {
      "Google\\Site_Kit\\": "includes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Google\\Site_Kit\\Tests\\": "tests/phpunit/includes"
    }
  },
  "config": {
    "platform": {
      "php": "5.4"
    }
  },
  "scripts": {
    "lint": "vendor/bin/phpcs",
    "lint-fix": "vendor/bin/phpcbf"
  }
}
