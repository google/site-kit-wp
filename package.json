    "element-closest": "^2.0.2",
    "eslint": "^6.4.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.2",
    "eslint-scope": "^5.0.0",
    "focus-trap-react": "^5.0.1",
