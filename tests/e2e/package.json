{
	"scripts": {
		"pretest": "./bin/local-env/env-check.sh && ./bin/test-docker-connectivity",
		"test": "cross-env NODE_OPTIONS=--experimental-abortcontroller WP_BASE_URL=http://localhost:9002 ./script.js",
		"pretest:playwright": "./bin/local-env/env-check.sh && ./bin/test-docker-connectivity",
		"test:playwright": "cross-env WP_BASE_URL=http://localhost:9002 playwright test --config=playwright.config.js --workers=4",
		"test:playwright:interactive": "cross-env WP_BASE_URL=http://localhost:9002 playwright test --config=playwright.config.js --workers=4 --headed",
		"test:interactive": "npm run test -- --puppeteer-interactive",
		"test:watch": "npm run test -- --watch",
		"test:watch:interactive": "npm run test -- --watch --puppeteer-interactive",
		"start": "./bin/local-env/start.sh",
		"stop": "./bin/local-env/stop.sh",
		"reset-site": "./bin/local-env/install-wordpress.sh --reset-site"
	},
	"config": {
		"puppeteer": {
			"skip": {
				"download": true
			}
		}
	},
	"dependencies": {
		"@playwright/test": "^1.48.2",
		"@wordpress/babel-preset-default": "^8.23.0",
		"@wordpress/e2e-test-utils": "^4.11.0",
		"@wordpress/e2e-test-utils-playwright": "^1.34.0",
		"@wordpress/jest-console": "^8.23.0",
		"amphtml-validator": "^1.0.35",
		"babel-jest": "^29.7.0",
		"babel-plugin-module-resolver": "^5.0.2",
		"cross-env": "^7.0.3",
		"dockerode": "^4.0.2",
		"expect-puppeteer": "^4.4.0",
		"jest": "^29.7.0",
		"jest-puppeteer": "^6.0.1",
		"jsdom": "^16.5.0",
		"lodash": "^4.17.21",
		"puppeteer": "^10.4.0"
	}
}
